{
  "manifest_version": 2,
  "name": "resendLeads  ",
  "version": "1.0",
  "description": "Helps",
  "permissions": [
    "activeTab",
    "storage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/js/index.js"],
      "css": ["/css/styles.css"]
    }
  ],
  "browser_action": {
    "default_icon": "/images/icon.jpg",
    "default_popup": "index.html"
  },
  "web_accessible_resources": ["fonts/*"]
}
